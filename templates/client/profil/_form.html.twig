{{ form_start(form) }}
    <div class="row">
        <div class="col-6">{{ form_row(form.nom) }}</div>
        <div class="col-6">{{ form_row(form.prenom) }}</div>
        <div class="col-12">{{ form_row(form.adresse) }}</div>
        <div class="col-4">{{ form_row(form.codePostal) }}</div>
        <div class="col-4">{{ form_row(form.ville) }}</div>
        <div class="col-4">{{ form_row(form.pays) }}</div>
        <div class="col-6">{{ form_row(form.email) }}</div>
        <div class="col-6">{{ form_row(form.societe) }}</div>
        <div class="col-6">{{ form_row(form.imageFile) }}</div>
        <div class="col-6">{{ form_row(form.telephone) }}</div>
        <div class="col-12"><button class="btn btn-info btn-block btn-sm">{{ button_label|default('Save') }}</button></div>
    </div>
    
    
{{ form_end(form) }}
